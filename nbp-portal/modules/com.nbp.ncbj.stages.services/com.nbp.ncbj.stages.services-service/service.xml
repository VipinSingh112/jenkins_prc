<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds"
	package-path="com.nbp.ncbj.stages.services">
	<namespace>NCBJ_STAGES</namespace>
	<!--<entity data-source="sampleDataSource" local-service="true" name="Foo" 
		remote-service="false" session-factory="sampleSessionFactory" table="foo" 
		tx-manager="sampleTransactionManager uuid="true""> -->
	<entity local-service="true" name="NcbjApplicationStages"
		remote-service="true" uuid="true" table="nbp_ncbj_app_stage">

		<!-- PK fields -->

		<column name="ncbjStagesApplicationId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->
		<column name="stageName" type="String"></column>
		<column name="duration" type="String"></column>
		<column name="stageStatus" type="String"></column>
		<column name="stageStartDate" type="Date"></column>
		<column name="stageEndDate" type="Date"></column>
		<column name="ncbjApplicationId" type="long"></column>
		<column name="caseId" type="String"></column>
		<column name="updateBySource" type="String"></column>
		<order by="desc">
			<order-column name="createDate" />
		</order>
		<finder name="getNcbj_AI"
			return-type="NcbjApplicationStages">
			<finder-column name="ncbjApplicationId"></finder-column>
		</finder>
		<finder name="getNcbj_By_CaseIdStageName"
			return-type="NcbjApplicationStages">
			<finder-column name="caseId"></finder-column>
			<finder-column name="stageName"></finder-column>
		</finder>
		<finder name="getNcbj_By_CaseIdStageStatus"
			return-type="NcbjApplicationStages">
			<finder-column name="caseId"></finder-column>
			<finder-column name="stageStatus"></finder-column>
			<finder-column name="stageName"></finder-column>
		</finder>
		<finder name="getNcbj_By_CaseIdAndStatus"
			return-type="NcbjApplicationStages">
			<finder-column name="caseId"></finder-column>
			<finder-column name="stageStatus"></finder-column>
		</finder>
		<finder name="getNcbj_By_CaseId" return-type="Collection">
			<finder-column name="caseId"></finder-column>
		</finder>
		<finder name="getNcbj_App_By_CaseId" return-type="NcbjApplicationStages">
			<finder-column name="caseId"></finder-column>
		</finder>
		<finder name="getQuarry_By_StageName" return-type="Collection">
			<finder-column name="stageName"></finder-column>
		</finder>
	</entity>
	<entity name="NcbjLicenceCertificate" local-service="true"
		table="nbp_ncbj_licence_certificate">

		<!-- PK fields -->

		<column name="ncbjLicenceCertificateId" primary="true"
			type="long">
		</column>

		<!-- Group instance -->

		<column name="groupId" type="long"></column>

		<!-- Audit fields -->

		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		
		<!-- Other Fields -->
		
		<column name="caseId" type="String"></column>
        <column name="licenceCertificate" type="String"></column>
        <column name="NameOfApplicant" type="String"></column>
        <column name="nameOfCertification" type="String"></column>
        <column name="scopeOfCertification" type="String"></column>
        <column name="issueDate" type="Date"></column>
        <column name="expirationDate" type="Date"></column>
        <column name="status" type="String"></column>
        <column name="docFileEntry" type="long"></column>
        <finder name="getNcbjLicenceCertiBy_CI" return-type="NcbjLicenceCertificate">
            <finder-column name="caseId"></finder-column>
        </finder>
        <finder name="getNcbjLicCertiBy_CI" return-type="Collection">
            <finder-column name="caseId"></finder-column>
        </finder>
    </entity>
	<entity name="NcbjAppBoardDecision" local-service="true"
		table="nbp_ncbj_board_decision">

		<!-- PK fields -->

		<column name="ncbjAppBoardDecisionId" primary="true"
			type="long">
		</column>

		<!-- Group instance -->

		<column name="groupId" type="long"></column>

		<!-- Audit fields -->

		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		
		<!-- Other Fields -->
		
		<column name="dateOfBoardDecision" type="Date"></column>
        <column name="committeeDecision" type="String"></column>
        <column name="caseId" type="String"></column>
         <order by="desc">
			<order-column name="createDate" />
		</order>
        <!-- Finder methods -->
		<finder name="getNcbj_CD_CI" return-type="NcbjAppBoardDecision">
			<finder-column name="caseId" />
		</finder>
		<finder name="getNcbj_By_CD" return-type="Collection">
			<finder-column name="committeeDecision" />
		</finder>
	</entity>
	<entity name="NcbjPaymentConfirmation" local-service="true"
		table="nbp_ncbj_payment_confirm">

		<!-- PK fields -->

		<column name="ncbjPaymentConfirmationId" primary="true"
			type="long">
		</column>

		<!-- Group instance -->

		<column name="groupId" type="long"></column>

		<!-- Audit fields -->

		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		
		<!-- Other Fields -->
		
		<column name="caseId" type="String"></column>
        <column name="dateReceived" type="Date"></column>
        <column name="amountReceived" type="String"></column>
        <column name="amountDue" type="String"></column>
        <column name="receiptNumber" type="String"></column>
        <column name="comment" type="String"></column>
        <column name="amountOutstanding" type="String"></column>
        <column name="dueDate" type="Date"></column>
        <column name="status" type="String"></column>
        <column name="docFileEntry" type="long"></column>
        <finder name="getNcbjPaymentConfBy_CI" return-type="NcbjPaymentConfirmation">
            <finder-column name="caseId"></finder-column>
        </finder>
        <finder name="getNcbjPayConfBy_CI" return-type="Collection">
            <finder-column name="caseId"></finder-column>
        </finder>
		
		
	</entity>
	<entity name="NcbjInspection" local-service="true"
		table="nbp_ncbj_inspection">

		<!-- PK fields -->

		<column name="ncbjInspectionId" primary="true" type="long"></column>

		<!-- Group instance -->

		<column name="groupId" type="long"></column>

		<!-- Audit fields -->

		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		
		<!-- Other Fields -->
	    <column name="caseId" type="String"></column>
        <column name="dateOfInspection" type="Date"></column>
		<column name="timeOfInspection" type="String" />
		<column name="location" type="String" />
		<column name="status"  type="String" />
		<column name="ncbjApplicationId"  type="long" />
		<column name="slotBookedByUser"  type="long" />
		
		 <order>
  			  <order-column name="createDate" order-by="desc" />
		</order>
        
        <finder name="getNcbjInspectionBy_CI" return-type="NcbjInspection">
            <finder-column name="caseId"></finder-column>
        </finder>
        
		<finder name="getNcbj_FBAI" return-type="Collection">
			<finder-column name="ncbjApplicationId" />
		</finder>
		<finder name="getNcbj_CaseId" return-type="Collection">
			<finder-column name="caseId" />
		</finder>
		<finder name="getNcbj_Status" return-type="Collection">
			<finder-column name="status" />
		</finder>
		<finder name="getNcbj_CaseIdStatus" return-type="Collection">
			<finder-column name="status" />
			<finder-column name="caseId" />
		</finder>
		<finder name="getNcbj_CaseIdAppId" return-type="Collection">
			<finder-column name="status" />
			<finder-column name="ncbjApplicationId" />
		</finder>
		<finder name="getInspecByStatus_SlotBook_AppId" return-type="NcbjInspection">
			<finder-column name="status" />
			<finder-column name="slotBookedByUser" />
			<finder-column name="ncbjApplicationId" />
		</finder>
	</entity>
	<entity name="NcbjApplicationCorrectiveAction" local-service="true"
		table="nbp_ncbj_corrective_act">

		<!-- PK fields -->

		<column name="ncbjAppCorrActionId" primary="true"
			type="long">
		</column>

		<!-- Group instance -->

		<column name="groupId" type="long"></column>

		<!-- Audit fields -->

		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		<column name="caseId" type="String"></column>
        <column name="preApprovalLetterId" type="long"></column>
         <order by="asc">
			<order-column name="createDate" />
		</order>
        <finder name="getPreAPprovalConditionBy_CI" return-type="NcbjApplicationCorrectiveAction">
          <finder-column name="caseId"></finder-column>
      </finder>
	</entity>
</service-builder>