<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="com.nbp.cannabis.collaboration.services">
	<namespace>CANNABIS_COLLABORATION</namespace>
	<!--<entity data-source="sampleDataSource" local-service="true" name="Foo" remote-service="false" session-factory="sampleSessionFactory" table="foo" tx-manager="sampleTransactionManager uuid="true"">-->
	<entity local-service="true" name="CannabisCollobartion" remote-service="true" uuid="true" table="nbp_cannabis_collaboration">

		<!-- PK fields -->

		<column name="cannabisCollaborationId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="cannabisStage" type="String" />
		<column name="userType" type="String" />
		<column name="comment" type="String" />
		<column name="status" type="String" />
		<column name="uploadedDocId" type="String" />
		<column name="caseId" type="String" />
		<column name="applicationId" type="String" />
		<column name="archived" type="int" />
		<!-- Order -->

		<order by="asc">
			<order-column name="createDate" />
		</order>

		<!-- Finder methods -->

		<finder name="caseId" return-type="Collection">
			<finder-column name="caseId" />
		</finder>
		<finder name="cannabisStage_CaseId" return-type="Collection">
			<finder-column name="caseId" />
			<finder-column name="cannabisStage" />
		</finder>
		<finder name="cannabisStage_CU" return-type="Collection">
			<finder-column name="caseId" />
			<finder-column name="cannabisStage" />
			<finder-column name="userType" />
		</finder>
		<finder name="cannabisStage_CUA" return-type="Collection">
			<finder-column name="caseId" />
			<finder-column name="cannabisStage" />
			<finder-column name="userType" />
			<finder-column name="archived" />
		</finder>
	</entity>
</service-builder>