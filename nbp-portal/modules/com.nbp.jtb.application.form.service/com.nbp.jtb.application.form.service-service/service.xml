	<entity local-service="true" name="JTBApplication"
		remote-service="true" uuid="true" table="nbp_jtb_applications">

		<!-- PK fields -->

		<column name="jtbApplicationId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long"></column>
		<column name="userId" type="long"></column>
		<column name="userName" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		<column name="entityId" type="String"></column>
		<!-- Other fields -->
		<column name="typeOfApplication" type="String"></column>
		<column name="categoryTypeValAccomo" type="String"></column>
		<column name="nonHotelSubCategory" type="String"></column>
		<column name="jtbTransactionTypeVal" type="String"></column>
		<column name="attractionCategoryTypeVal" type="String"></column>
		<column name="domesticToursType" type="String"></column>
		<column name="otherCategories" type="String"></column>
		<column name="operatorName" type="String"></column>
		<column name="dateOfIssue" type="Date"></column>
		<column name="dateOfExp" type="Date"></column>
		<column name="status" type="int"></column>
		<column name="applicationNumber" type="String"></column>
		<column name="expiredLicenseAppNumber" type="String"></column>
		<column name="caseId" type="String"></column>
		<column name="icmDocumentsPath" type="String" />
		<column name="pdfFileEntryId" type="long" />
		<column name="dateOfExpiration" type="Date" />
		<column name="appCurrentStageName" type="String" />

		 <order by="desc">
			<order-column name="createDate" />
		</order>

		<finder name="getJTB_ByS_U" return-type="Collection">
			<finder-column name="userId"></finder-column>
			<finder-column name="status"></finder-column>
		</finder>
		<finder name="getJTB_By_Status" return-type="Collection">
			<finder-column name="status"></finder-column>
		</finder>
		<finder name="getJTB_By_AppNum" return-type="JTBApplication">
			<finder-column name="applicationNumber"></finder-column>
		</finder>
		<finder name="getJTB_By_RT_AppNo" return-type="JTBApplication">
			<finder-column name="jtbTransactionTypeVal"></finder-column>
			<finder-column name="expiredLicenseAppNumber"></finder-column>
		</finder>
		<finder name="getJTB_ByCaseId" return-type="JTBApplication">
			<finder-column name="caseId"></finder-column>
		</finder>
		<finder name="getJTB_By_AppType_Status" return-type="Collection">
			<finder-column name="status"></finder-column>
			<finder-column name="typeOfApplication"></finder-column>
		</finder>
	</entity>
